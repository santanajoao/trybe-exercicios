{
  "name": "dia-01-interface-com-o-banco",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "start": "env $(cat .env) node src/server.js",
    "dev": "env $(cat .env) nodemon src/server.js",
    "sequelize:create": "env $(cat .env) npx sequelize db:create",
    "mysql:start": "docker container run --name mysql -e MYSQL_ROOT_PASSWORD=password -d -p 3306:3306 mysql:8.0.29",
    "mysql:stop": "docker container rm -f mysql",
    "migrate": "env $(cat .env) npx sequelize db:migrate",
    "migrate:undo": "env $(cat .env) npx sequelize db:migrate:undo",
    "seed": "env $(cat .env) npx sequelize db:seed:all",
    "seed:undo": "env $(cat .env) npx sequelize db:seed:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "4.18.2",
    "express-async-errors": "^3.1.1",
    "mysql2": "3.2.4",
    "sequelize": "6.31.1"
  },
  "devDependencies": {
    "nodemon": "2.0.22",
    "sequelize-cli": "6.6.0"
  }
}
